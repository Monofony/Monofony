{
    "name": "mobizel/monofony",
    "description": "Starter project for Symfony projects",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.4.8",
        "eightpoints/guzzle-bundle": "^7.3",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "fzaninotto/Faker": "^1.5",
        "guzzlehttp/guzzle": "^6.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "knplabs/doctrine-behaviors": "~1.1",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/block-bundle": "^3.3",
        "sylius-labs/sensio-distribution-bundle": "^6.0",
        "sylius/customer-bundle": "^1.3",
        "sylius/fixtures-bundle": "^1.3",
        "sylius/grid-bundle": "^1.3",
        "sylius/mailer-bundle": "^1.3",
        "sylius/resource-bundle": "^1.3",
        "sylius/ui-bundle": "^1.3",
        "sylius/user-bundle": "^1.3",
        "symfony/flex": "^1.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/proxy-manager-bridge": "^4.2",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "^4.2",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "friends-of-behat/context-service-extension": "^1.2",
        "friends-of-behat/cross-container-extension": "^1.1",
        "friends-of-behat/page-object-extension": "^0.2.1",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.2",
        "friends-of-behat/variadic-extension": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.13",
        "infection/infection": "^0.10.6",
        "lakion/mink-debug-extension": "^1.2",
        "lchrusciel/api-test-case": "^3.1",
        "leanphp/phpspec-code-coverage": "^4.2",
        "nikic/php-parser": "^4.1",
        "phpspec/phpspec": "^4.0",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-shim": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-webmozart-assert": "^0.10",
        "se/selenium-server-standalone": " ^2.52",
        "symfony/debug-bundle": "^4.1",
        "symfony/web-profiler-bundle": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
