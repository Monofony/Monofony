{
    "name": "mobizel/monofony",
    "description": "Starter project for Symfony projects",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",

        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "eightpoints/guzzle-bundle": "^7.3",
        "fzaninotto/Faker": "^1.5",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "knplabs/doctrine-behaviors": "~1.1",
        "sonata-project/block-bundle": "^3.3",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "3.4.*",
        "symfony/monolog-bundle": "~2.4",
        "sylius/customer-bundle": "^1.2",
        "sylius/fixtures-bundle": "^v1.2",
        "sylius/grid-bundle": "^1.2",
        "sylius/mailer-bundle": "^1.2",
        "sylius/resource-bundle": "^1.2",
        "sylius/ui-bundle": "^1.2",
        "sylius/user-bundle": "^1.2",
        "winzou/state-machine-bundle": "~0.2",
        "sylius-labs/sensio-distribution-bundle": "^6.0",
        "sensio/framework-extra-bundle": "^5.1"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/symfony2-extension": "~2.0@dev",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink": "^1.7@dev",
        "lakion/api-test-case": "^1.1",
        "phpspec/phpspec": "^4.0",
        "se/selenium-server-standalone": " ^2.52",
        "malukenho/kawaii-gherkin": "^0.1.3",
        "phpstan/phpstan-shim": "^0.9.2",
        "friends-of-behat/context-service-extension": "^1.2",
        "friends-of-behat/cross-container-extension": "^1.1",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.2",
        "friends-of-behat/variadic-extension": "^1.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
