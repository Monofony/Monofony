{
    "name": "mobizel/monofony",
    "description": "Starter project for Symfony projects",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",

        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "eightpoints/guzzle-bundle": "^7.3",
        "fzaninotto/Faker": "^1.5",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "knplabs/doctrine-behaviors": "~1.1",
        "sonata-project/block-bundle": "^3.3",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "sylius/customer-bundle": "^1.2.1",
        "sylius/fixtures-bundle": "^v1.2.1",
        "sylius/grid-bundle": "^1.2.1",
        "sylius/mailer-bundle": "^1.2.1",
        "sylius/resource-bundle": "^1.2.1",
        "sylius/ui-bundle": "^1.2.1",
        "sylius/user-bundle": "^1.2.1",
        "sylius-labs/sensio-distribution-bundle": "^6.0",
        "sensio/framework-extra-bundle": "^5.1"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink": "^1.7@dev",
        "behat/symfony2-extension": "^2.1",
        "friends-of-behat/context-service-extension": "^1.2",
        "friends-of-behat/cross-container-extension": "^1.1",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.2",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/api-test-case": "^3.1",
        "phpspec/phpspec": "^4.0",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-shim": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-webmozart-assert": "^0.10",
        "se/selenium-server-standalone": " ^2.52"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
