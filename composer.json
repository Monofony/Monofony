{
    "name": "monofony/monofony",
    "description": "Starter project for Symfony projects",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Collector\\": "src/Core/recipe/src/Collector/",
            "App\\Context\\": "src/Monofony/Bundle/CoreBundle/Recipe/src/Context/",
            "App\\Command\\": "src/Core/recipe/src/Command/",
            "App\\Dashboard\\": "src/Admin/recipe/src/Dashboard/",
            "App\\Entity\\": "src/Core/recipe/src/Entity/",
            "App\\EventListener\\": "src/Core/recipe/src/EventListener/",
            "App\\EventSubscriber\\": "src/Core/recipe/src/EventSubscriber/",
            "App\\Fixture\\": "src/Fixtures/recipe/src/Fixture/",
            "App\\Form\\": "src/Core/recipe/src/Form/",
            "App\\Formatter\\": "src/Core/recipe/src/Formatter/",
            "App\\Installer\\": "src/Core/recipe/src/Installer/",
            "App\\Installer\\Checker\\": "src/Core/recipe/src/Installer/Checker/",
            "App\\Migrations\\": "src/Core/recipe/src/Migrations/",
            "App\\Repository\\": "src/Core/recipe/src/Repository/",
            "App\\Validator\\": "src/Core/recipe/src/Validator/",
            "Monofony\\Bundle\\": "src/Monofony/Bundle/",
            "Monofony\\Component\\": "src/Monofony/Component/"
        },
        "classmap": [
            "src/Admin/recipe/src/DependencyInjection/Builder/BackendContainerBuilder.php",
            "src/Front/recipe/src/DependencyInjection/Builder/FrontendContainerBuilder.php",
            "src/Admin/recipe/src/DependencyInjection/Compiler/ConfigureDashboardStatisticsProviderPass.php",
            "src/Admin/recipe/src/Controller/DashboardController.php",
            "src/Admin/recipe/src/Menu/AdminMenuBuilder.php",
            "src/Front/recipe/src/Menu/AccountMenuBuilder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Tests\\Behat\\": "src/Core/recipe/tests/Behat/",
            "App\\Tests\\Behat\\Behaviour\\": "src/Core/recipe/tests/Behat/Behaviour/",
            "App\\Tests\\Behat\\Context\\Cli\\": "src/Core/recipe/tests/Behat/Context/Cli/",
            "App\\Tests\\Behat\\Context\\Hook\\": "src/Core/recipe/tests/Behat/Context/Hook/",
            "App\\Tests\\Behat\\Context\\Setup\\": "src/Core/recipe/tests/Behat/Context/Setup/",
            "App\\Tests\\Behat\\Context\\Transform\\": "src/Core/recipe/tests/Behat/Context/Transform/",
            "App\\Tests\\Behat\\Context\\Ui\\": "src/Core/recipe/tests/Behat/Context/Ui/",
            "App\\Tests\\Behat\\Context\\Ui\\Backend\\": "src/Admin/recipe/tests/Behat/Context/Ui/Backend/",
            "App\\Tests\\Behat\\Context\\Ui\\Frontend\\": "src/Front/recipe/tests/Behat/Context/Ui/Frontend/",
            "App\\Tests\\Behat\\Element\\Backend\\": "src/Admin/recipe/tests/Behat/Element/Backend/",
            "App\\Tests\\Behat\\Exception\\": "src/Core/recipe/tests/Behat/Exception/",
            "App\\Tests\\Behat\\Page\\Backend\\": "src/Admin/recipe/tests/Behat/Page/Backend/",
            "App\\Tests\\Behat\\Page\\Frontend\\": "src/Front/recipe/tests/Behat/Page/Frontend/",
            "App\\Tests\\Behat\\Service\\": "src/Core/recipe/tests/Behat/Service/",
            "App\\Tests\\Controller\\": "src/Api/recipe/tests/Controller/"
        }
    },
    "require": {
        "php": "^7.2",
        "api-platform/api-pack": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.4.8",
        "eightpoints/guzzle-bundle": "^7.3",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/doctrine-behaviors": "~1.1",
        "liip/imagine-bundle": "^2.2",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0",
        "sonata-project/block-bundle": "^3.3",
        "sylius/customer-bundle": "^1.6",
        "sylius/fixtures-bundle": "^1.4",
        "sylius/grid-bundle": "^1.6",
        "sylius/mailer-bundle": "^1.4",
        "sylius/resource-bundle": "^1.6",
        "sylius/ui-bundle": "^1.6",
        "sylius/user-bundle": "^1.6",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.7",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.13",
        "infection/infection": "^0.13",
        "lakion/mink-debug-extension": "^1.2",
        "lchrusciel/api-test-case": "^3.1",
        "phpspec/phpspec": "^5.0",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpstan/phpstan-nette": "^0.11",
        "phpstan/phpstan-symfony": "^0.11",
        "phpstan/phpstan-webmozart-assert": "^0.11",
        "proget-hq/phpstan-phpspec": "^0.2.0",
        "psr/event-dispatcher": "^1.0",
        "se/selenium-server-standalone": " ^2.52",
        "symfony/debug-bundle": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symplify/easy-coding-standard": "^6.0||^7.0",
        "symplify/monorepo-builder": "^7.0",
        "vimeo/psalm": "^3.4"
    },
    "conflict": {
        "symfony/symfony": "*",
        "symplify/package-builder": "^7.2",
        "symplify/smart-file-system": "^7.2",
        "symplify/monorepo-builder": "^7.2"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "fix": [
            "@php vendor/bin/ecs check src --fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
