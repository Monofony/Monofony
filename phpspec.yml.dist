suites:
    CoreBundle: { spec_path: src/Bundle/CoreBundle, src_path: src/Bundle/CoreBundle }
    Core: { spec_path: src/Core/recipe, src_path: src/Core/recipe/src }
    Backend: { spec_path: src/Admin/recipe, src_path: src/Admin/recipe/src }

extensions:
    LeanPHP\PhpSpec\CodeCoverage\CodeCoverageExtension:
        whitelist:
            - src/Core/recipe/src
            - src/Admin/recipe/src
