{
    "timeout": 10,
    "source": {
        "directories": [
            "src/monofony-core/recipe/src",
            "src/monofony-backend/recipe/src",
            "src/monofony-frontend/recipe/src",
            "src/monofony-fixtures/recipe/src"
        ],
        "excludes": [
            "Command",
            "Controller",
            "DependencyInjection",
            "Formatter",
            "Menu",
            "Migrations",
            "Fixture",
            "Form/Type",
            "Kernel.php"
        ]
    },
    "logs": {
        "text": "infection-log.txt"
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": false,
        "NotIdenticalNotEqual": false
    },
    "testFramework":"phpspec"
}
