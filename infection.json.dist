{
    "timeout": 10,
    "source": {
        "directories": [
            "recipes/monofony-core/src",
            "recipes/monofony-backend/src",
            "recipes/monofony-frontend/src",
            "recipes/monofony-fixtures/src"
        ],
        "excludes": [
            "Command",
            "Controller",
            "DependencyInjection",
            "Formatter",
            "Menu",
            "Migrations",
            "Fixture",
            "Form/Type",
            "Kernel.php"
        ]
    },
    "logs": {
        "text": "infection-log.txt"
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": false,
        "NotIdenticalNotEqual": false
    },
    "testFramework":"phpspec"
}
