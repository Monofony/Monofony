{
    "name": "monofony/sylius-user-bridge",
    "type": "symfony-bridge",
    "description": "Monofony Sylius User Bridge",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "monofony/core-contracts": "dev-master",
        "monofony/user-bundle": "dev-master",
        "sylius/customer": "^1.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": { "Monofony\\Bridge\\SyliusUser\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
