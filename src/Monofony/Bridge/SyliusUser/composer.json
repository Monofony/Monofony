{
    "name": "monofony/sylius-user-bridge",
    "type": "symfony-bridge",
    "description": "Monofony Sylius User Bridge",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "monofony/core-contracts": "^0.9@dev",
        "sylius/user": "^1.12 || ^1.12@dev",
        "sylius/user-bundle": "^1.12 || ^1.12@dev",
        "monofony/customer": "^1.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Monofony\\Bridge\\SyliusUser\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
