{
    "name": "monofony/fos-oauth-server-bridge",
    "type": "symfony-bridge",
    "description": "Monofony FOS OAuth Server Bridge",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "friendsofsymfony/oauth-server-bundle": "^1.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": { "Monofony\\Bridge\\FOS\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-version": "0.4"
    }
}
