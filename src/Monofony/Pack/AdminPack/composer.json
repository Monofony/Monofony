{
    "name": "monofony/admin-pack",
    "type": "symfony-pack",
    "description": "Monofony admin pack.",
    "license": "MIT",
    "require": {
        "php": ">=7.4 || ^8.0",
        "monofony/admin": "^0.7",
        "monofony/admin-contracts": "^0.7",
        "monofony/core-contracts": "^0.7",
        "monofony/ui-bundle": "^1.10",
        "sonata-project/block-bundle": "^4.2",
        "sylius/grid-bundle": "^1.9",
        "symfony/asset": "^5.3 || ^6.0",
        "symfony/routing": "^5.3 || ^6.0",
        "symfony/templating": "^5.3 || ^6.0",
        "twig/twig": "^2.7",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "friends-of-behat/page-object-extension": "^0.3",
        "monofony/behat-bridge": "^0.7",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/.recipe/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": ".recipe/src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    }
}
