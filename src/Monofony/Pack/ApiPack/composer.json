{
    "name": "monofony/api-pack",
    "type": "symfony-pack",
    "description": "A pack for Monofony.",
    "license": "MIT",
    "require": {
        "api-platform/api-pack": "^1.2",
        "monofony/core-contracts": "^0.4",
        "symfony/messenger": "^4.4 || ^5.1",
        "sylius/resource": "^1.6",
        "sylius/user-bundle": "^1.6",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "lchrusciel/api-test-case": "^3.1"
    },
    "autoload": {
        "exclude-from-classmap": ["/.recipe/"]
    },
    "replace": {
        "monofony/core-contracts": "self.version"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
