{
    "name": "monofony/api-pack",
    "type": "symfony-pack",
    "description": "A pack for Monofony.",
    "license": "MIT",
    "require": {
        "api-platform/core": "^2.6",
        "api-platform/api-pack": "^1.2",
        "monofony/api-contracts": "^0.7",
        "monofony/core-contracts": "^0.7",
        "monofony/user": "^1.10",
        "monofony/user-bundle": "^1.10",
        "psr/event-dispatcher": "^1.0",
        "sylius/resource": "^1.8",
        "symfony/messenger": "^5.3 || ^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "lchrusciel/api-test-case": "^5.1"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/.recipe/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    }
}
