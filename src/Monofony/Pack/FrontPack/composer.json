{
    "name": "monofony/front-pack",
    "type": "symfony-pack",
    "description": "Monofony front pack.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "monofony/core-bundle": "^0.8",
        "monofony/front-contracts": "^0.8",
        "monofony/core-contracts": "^0.8",
        "monofony/ui-bundle": "^1.10",
        "symfony/asset": "^5.3 || ^6.0",
        "symfony/routing": "^5.3 || ^6.0",
        "symfony/templating": "^5.3 || ^6.0",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "monofony/behat-bridge": "^0.8"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/.recipe/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
