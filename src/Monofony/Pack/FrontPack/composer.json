{
    "name": "monofony/front-pack",
    "type": "symfony-pack",
    "description": "Monofony front pack.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "monofony/behat-bridge": "^0.8",
        "monofony/core-bundle": "^0.8",
        "monofony/front-contracts": "^0.8",
        "monofony/front-meta": "^0.8",
        "monofony/core-contracts": "^0.8",
        "sylius/ui-bundle": "^1.11",
        "phpstan/phpstan": "^1.5",
        "symfony/asset": "5.4.*",
        "symfony/routing": "5.4.*",
        "symfony/templating": "5.4.*",
        "twig/twig": "^2.7",
        "zenstruck/foundry": "^1.14"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5",
        "monofony/behat-bridge": "^0.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
