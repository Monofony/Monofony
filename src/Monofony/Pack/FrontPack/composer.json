{
    "name": "monofony/front-pack",
    "type": "symfony-pack",
    "description": "Monofony front pack.",
    "license": "MIT",
    "require": {
        "php": ">=7.4 || ^8.0",
        "monofony/core-bundle": "^0.6",
        "monofony/front-contracts": "^0.6",
        "monofony/core-contracts": "^0.6",
        "monofony/ui-bundle": "^1.10",
        "symfony/asset": "^4.4 || ^5.1",
        "symfony/routing": "^4.4 || ^5.1",
        "symfony/templating": "^4.4 || ^5.1",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "monofony/behat-bridge": "^0.6"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/.recipe/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
