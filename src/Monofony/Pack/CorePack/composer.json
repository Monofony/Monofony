{
    "name": "monofony/core-pack",
    "type": "symfony-pack",
    "description": "Monofony core pack.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "behat/behat": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/migrations": "3.2.*",
        "doctrine/orm": "^2.5",
        "fakerphp/faker": "^1.14",
        "liip/imagine-bundle": "^2.2",
        "monofony/behat-bridge": "^0.9@dev",
        "monofony/core-bundle": "^0.9@dev",
        "monofony/core-contracts": "^0.9@dev",
        "monofony/core-meta": "^0.9@dev",
        "monofony/sylius-user-bridge": "^0.9@dev",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-webmozart-assert": "^1.1",
        "sylius/customer-bundle": "^1.12@beta",
        "sylius/mailer": "^2.0",
        "sylius/resource-bundle": "^1.10@alpha",
        "sylius/user-bundle": "^1.12@beta",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/process": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/translation": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "vich/uploader-bundle": "^1.8",
        "zenstruck/foundry": "^1.14"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "monofony/behat-bridge": "^0.9@dev",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-webmozart-assert": "^1.1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
