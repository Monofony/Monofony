{
    "name": "monofony/core-pack",
    "type": "symfony-pack",
    "description": "Monofony core pack.",
    "license": "MIT",
    "require": {
        "php": ">=7.4 || ^8.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.4.8",
        "fakerphp/faker": "^1.14",
        "liip/imagine-bundle": "^2.2",
        "monofony/core-bundle": "^0.5",
        "monofony/core-contracts": "^0.5",
        "monofony/fos-oauth-server-bridge": "^0.5",
        "monofony/sylius-user-bridge": "^0.5",
        "monofony/customer-bundle": "^1.9",
        "sylius/fixtures-bundle": "^1.7",
        "sylius/mailer": "^1.5",
        "sylius/resource": "^1.6",
        "sylius/resource-bundle": "^1.8",
        "monofony/user": "^1.9",
        "monofony/user-bundle": "^1.9",
        "symfony/process": "^4.4 || ^5.1",
        "symfony/serializer": "^4.4 || ^5.1",
        "symfony/translation": "^4.4 || ^5.1",
        "symfony/validator": "^4.4 || ^5.1",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "monofony/behat-bridge": "^0.5",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/.recipe/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
