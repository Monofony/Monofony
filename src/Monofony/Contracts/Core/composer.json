{
    "name": "monofony/core-contracts",
    "type": "library",
    "description": "Generic abstractions related to Core",
    "license": "MIT",
    "require": {
        "php": ">=7.4 || ^8.0",
        "monofony/customer": "^1.10",
        "monofony/user": "^1.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Monofony\\Contracts\\Core\\": ""
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    }
}
