{
    "name": "monofony/api-contracts",
    "type": "library",
    "description": "Generic abstractions related to Api",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7 || ^3.1",
        "symfony/serializer": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Monofony\\Contracts\\Api\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5"
    }
}
