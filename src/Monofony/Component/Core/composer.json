{
    "name": "monofony/core",
    "type": "library",
    "description": "Monofony core.",
    "license": "MIT",
    "require": {
        "php": ">=7.4 || ^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Monofony\\Component\\Core\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    }
}
