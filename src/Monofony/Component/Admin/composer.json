{
    "name": "monofony/admin",
    "type": "library",
    "description": "Monofony admin.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "knplabs/knp-menu": "^3.1",
        "monofony/admin-contracts": "^0.9@dev",
        "monofony/core": "^0.9@dev",
        "symfony/routing": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Monofony\\Component\\Admin\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5",
        "friends-of-behat/page-object-extension": "^0.3"
    }
}
