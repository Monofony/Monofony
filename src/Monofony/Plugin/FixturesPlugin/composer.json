{
    "name": "monofony/fixtures-plugin",
    "type": "symfony-bundle",
    "description": "Monofony fixtures plugin.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "monofony/core-bundle": "^0.1",
        "sylius/fixtures-bundle": "^1.4"
    },
    "autoload": {
        "psr-4": { "Monofony\\Plugin\\FixturesPlugin\\": "" },
        "exclude-from-classmap": ["Recipe/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
