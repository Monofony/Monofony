{
    "name": "monofony/front-bundle",
    "type": "symfony-bundle",
    "description": "Monofony front bundle.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "monofony/core-bundle": "^0.3"
    },
    "autoload": {
        "psr-4": { "Monofony\\Bundle\\FrontBundle\\": "" },
        "exclude-from-classmap": ["Recipe/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
