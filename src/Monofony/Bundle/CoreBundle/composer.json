{
    "name": "monofony/core-bundle",
    "type": "symfony-bundle",
    "description": "Monofony core bundle.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.4.8",
        "eightpoints/guzzle-bundle": "^7.3",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/doctrine-behaviors": "~1.1",
        "liip/imagine-bundle": "^2.2",
        "monofony/admin-contracts": "^0.4",
        "monofony/sylius-bridge": "^0.4",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0",
        "sonata-project/block-bundle": "^3.3",
        "sylius/customer-bundle": "^1.6",
        "sylius/fixtures-bundle": "^1.4",
        "sylius/grid-bundle": "^1.6",
        "sylius/mailer-bundle": "^1.4",
        "sylius/resource-bundle": "^1.6",
        "sylius/ui-bundle": "^1.6",
        "sylius/user-bundle": "^1.6",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-php80": "^1.15",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.7",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": { "Monofony\\Bundle\\CoreBundle\\": "" },
        "exclude-from-classmap": [".recipe/"]
    },
    "replace": {
        "monofony/admin-contracts": "self.version",
        "monofony/sylius-bridge": "self.version"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-version": "0.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    }
}
